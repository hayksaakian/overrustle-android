<?xml version="1.0" encoding="utf-8"?>
<layer-list
	xmlns:android="http://schemas.android.com/apk/res/android">

	<item>
		<shape
			android:shape="rectangle" >

			<stroke
				android:width="1dp"
				android:color="#551D1D1D" /> 

			<gradient
				android:type="linear"
				android:angle="270"
				android:gradientRadius="100"
				android:centerY="0.00"
				android:centerX="0.50"
				android:endColor="#CC111111"
				android:centerColor="#EE222222"
				android:startColor="#EE222222" />

		</shape>
	</item>

</layer-list>
